<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<StackPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="100.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.MessageBoxController">
  <children>
    <Text fx:id="message" strokeType="OUTSIDE" strokeWidth="0.0" text="Team name is already taken" textAlignment="CENTER" wrappingWidth="180.0" StackPane.alignment="TOP_CENTER">
      <StackPane.margin>
        <Insets />
      </StackPane.margin>
      <font>
        <Font name="System Bold" size="14.0" />
      </font>
    </Text>
    <Button mnemonicParsing="false" onAction="#onOKButtonClicked" text="OK" StackPane.alignment="BOTTOM_CENTER" />
  </children>
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
  </padding>
</StackPane>
